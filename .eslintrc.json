{
    "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "max-len": ["error", { "code": 140 }],
        "arrow-body-style": "off",
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off"
    }
}
